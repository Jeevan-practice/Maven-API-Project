pipeline {
    agent any
 tools {
        maven 'maven-tool'
        jdk 'java-17'
    }
    stages {
        stage("Checkout Demo code") {
            steps {
                deleteDir()
                sh 'git clone https://github.com/Jeevan-practice/sample-maven.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
                dir('sample-maven') {
                    sh 'mvn clean package'
                }
            }
        }
}
}





